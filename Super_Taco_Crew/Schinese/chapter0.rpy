translate Schinese chapter0_646fcd80:

    # "He called James."
    "他打给了James。"

# game/scripts/chapter0.rpy:1218
translate Schinese chapter0_a20cefa7_1:

    # "..."
    "..."

# game/scripts/chapter0.rpy:1220
translate Schinese chapter0_389aaa78:

    # "-ring- -ring-"
    "-铃--铃-"

